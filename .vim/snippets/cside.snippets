# for
snippet for
	for i:=1 to {1:count} do
		{2:code}
# forr
snippet forr
	for i:=1 to {1:count} do begin
		{2:code}
	end;
# iff
snippet iff
	if ({1:condition}) then begin
		{2:code}
	end;
# if
snippet if
	if ({1:condition}) then
		{2:code}
# while
snippet while
	while ({1:condition}) then
		{2:code}
# whilee
snippet whilee
	while ({1:condition}) then begin
		{2:code}
	end;
# case
	case {1} of
		{2:case1} : begin
		end;
	end;
